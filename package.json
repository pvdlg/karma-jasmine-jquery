{
  "name": "@metahub/karma-jasmine-jquery",
  "description": "A Karma plugin for jasmine-jquery",
  "version": "0.0.0-development",
  "author": "Pierre Vanduynslager (https://github.com/pvdlg)",
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "4"
          }
        }
      ]
    ]
  },
  "bugs": {
    "url": "https://github.com/pvdlg/karma-jasmine-jquery/issues"
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "codecov": "^3.0.0",
    "conventional-changelog-metahub": "^1.0.1",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-sort-class-members": "^1.2.0",
    "jasmine-core": "^2.8.0",
    "jasmine-jquery": "^2.1.1",
    "jquery": "^3.2.1",
    "karma": "^2.0.0",
    "karma-babel-preprocessor": "^7.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-jquery": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "nyc": "^11.1.0",
    "p-event": "^1.3.0",
    "prettier": "^1.5.3",
    "rimraf": "^2.6.1",
    "rollup": "^0.53.0",
    "rollup-plugin-commonjs": "^8.2.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-re": "^1.0.6",
    "semantic-release": "^12.0.0",
    "xo": "^0.18.2"
  },
  "engines": {
    "node": ">=4"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/pvdlg/karma-jasmine-jquery#readme",
  "keywords": [
    "jasmine",
    "jasmine-jquery",
    "jquery",
    "karma",
    "karma-framework",
    "karma-jasmine",
    "karma-plugin"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "nyc": {
    "include": [
      "lib/*.js",
      "!lib/*.bundle.js"
    ],
    "reporter": [
      "json",
      "text",
      "html"
    ],
    "all": true
  },
  "peerDependencies": {
    "jasmine-core": "*",
    "jasmine-jquery": "*",
    "karma": "*",
    "karma-jasmine": "*"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "bracketSpacing": false,
    "trailingComma": "es5"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "analyzeCommits": {
      "preset": "metahub",
      "releaseRules": "conventional-changelog-metahub/release-rules"
    },
    "generateNotes": {
      "preset": "metahub"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pvdlg/karma-jasmine-jquery.git"
  },
  "scripts": {
    "codecov": "codecov -f coverage/coverage-final.json",
    "compile": "rimraf lib && babel src --only index.js --source-maps --out-dir lib && rollup -c build/rollup.config.js",
    "lint": "xo",
    "prepare": "npm run compile",
    "prepublishOnly": "npm run compile",
    "pretest": "npm run compile && npm run lint",
    "semantic-release": "semantic-release",
    "test": "nyc ava -v"
  },
  "xo": {
    "extends": [
      "plugin:prettier/recommended"
    ]
  }
}
