{
  "name": "@metahub/karma-jasmine-jquery",
  "description": "A Karma plugin for jasmine-jquery",
  "version": "1.0.0",
  "author": "Pierre Vanduynslager",
  "bugs": {
    "url": "https://github.com/vanduynslagerp/karma-jasmine-jquery/issues"
  },
  "peerDependencies": {
    "karma-jasmine": "*",
    "jasmine-jquery": "*",
    "jasmine-core": "*",
    "karma": "*"
  },
  "devDependencies": {
    "@metahub/eslint-config": "^1.6.0",
    "ava": "^0.21.0",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "codecov": "^2.2.0",
    "eslint": "^4.3.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-ava": "^4.2.1",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-eslint-comments": "^1.0.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jasmine": "^2.8.2",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-sort-class-members": "^1.1.1",
    "jasmine-core": "^2.7.0",
    "jasmine-jquery": "^2.1.1",
    "jquery": "^3.2.1",
    "karma": "^1.7.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-jasmine": "^1.1.0",
    "karma-jquery": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "nyc": "^11.0.3",
    "p-event": "^1.3.0",
    "prettier": "^1.5.3",
    "rimraf": "^2.6.1",
    "rollup": "^0.46.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-re": "^1.0.6"
  },
  "engines": {
    "node": ">=4"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/vanduynslagerp/karma-jasmine-jquery#readme",
  "keywords": [
    "karma",
    "karma-plugin",
    "karma-framework",
    "jasmine",
    "jquery",
    "karma-jasmine",
    "jasmine-jquery"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vanduynslagerp/karma-jasmine-jquery.git"
  },
  "scripts": {
    "clean": "rimraf test/fixtures/.tmp && rimraf coverage && rimraf .nyc_output",
    "compile": "rimraf lib && babel src --only index.js --source-maps --out-dir lib && rollup -c build/rollup.config.js -o lib/jasmine-jquery.bundle.js",
    "lint": "eslint src test package.json",
    "pretest": "npm run clean && npm run compile && npm run lint",
    "test": "nyc ava -v && nyc report --reporter=lcov",
    "codecov": "codecov",
    "prepare": "npm run compile",
    "prepublishOnly": "npm run test"
  }
}
